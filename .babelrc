{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    [
      "import",
      [
        {
          "libraryName": "antd",
          "libraryDirectory": "lib",
          "style": "css"
        }
      ]
    ],
    "transform-decorators-legacy"
  ]
}
